  <Variable
    Name="srcCust"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT AC.CUSTOMER_NUMBER,
    AC.CUSTOMER_NUMBER  ||' '|| AC.CUSTOMER_NAME AS CUSTOMER_FULL_NAME,
    AC.CUSTOMER_NUMBER  ||' '|| AC.CUSTOMER_NAME AS CUSTOMER_SEARCH
FROM APPS.AR_CUSTOMERS AC
WHERE AC.ATTRIBUTE3 = NVL(:pRegion,AC.ATTRIBUTE3)
AND AC.ATTRIBUTE4 = NVL(:pArea,AC.ATTRIBUTE4)
AND AC.ATTRIBUTE5 = NVL(:pLoc,AC.ATTRIBUTE5)
AND AC.ATTRIBUTE8 = NVL(:pType,AC.ATTRIBUTE8)
AND AC.ATTRIBUTE1 = NVL(:pGroup,AC.ATTRIBUTE1)
AND AC.STATUS = 'A']]>
  </SQLQuery>
