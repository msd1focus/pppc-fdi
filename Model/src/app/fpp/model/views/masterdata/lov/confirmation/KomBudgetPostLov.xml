  <SQLQuery>
    <![CDATA[SELECT LovbudgetcustomerView.BUDGET_CUST_HDR_ID, 
       LovbudgetcustomerView.CUSTOMER_ID, 
       LovbudgetcustomerView.BUDGET_YEAR, 
       LovbudgetcustomerView.BUDGET_CUSTOMER_ID, 
       LovbudgetcustomerView.KODE_POSTING, 
       UPPER(FcsViewItemMasterCategory.ITEM || '.' || FcsViewItemMasterCategory.ITEM_DESCRIPTION || '.' || FcsViewCategCombDesc.COMBINATION_DESCRIPTION ||'.'||LovbudgetcustomerView.BUDGET_YEAR) AS COMBINATION_DESCRIPTION, 
       FcsViewCategCombDesc.COMBINATION_CATEGORY, 
       LovbudgetcustomerView.KOMBINASI_CODE, 
       LovbudgetcustomerView.BUDGET_TYPE
FROM LOVBUDGETCUSTOMER_VIEW LovbudgetcustomerView, APPS.FCS_VIEW_CATEG_COMB_DESC FcsViewCategCombDesc, APPS.FCS_VIEW_ITEM_MASTER_CATEGORY FcsViewItemMasterCategory
WHERE LovbudgetcustomerView.KOMBINASI_CODE =FcsViewCategCombDesc.COMBINATION_CATEGORY(+)
AND FcsViewItemMasterCategory.ITEM = LovbudgetcustomerView.KODE_POSTING 
AND LovbudgetcustomerView.BUDGET_TYPE = 'POSTING'
GROUP BY LovbudgetcustomerView.BUDGET_CUST_HDR_ID, 
       LovbudgetcustomerView.CUSTOMER_ID, 
       LovbudgetcustomerView.BUDGET_YEAR, 
       LovbudgetcustomerView.BUDGET_CUSTOMER_ID, 
       LovbudgetcustomerView.KODE_POSTING, 
       FcsViewItemMasterCategory.ITEM,
       FcsViewItemMasterCategory.ITEM_DESCRIPTION,
       FcsViewCategCombDesc.COMBINATION_DESCRIPTION, 
       FcsViewCategCombDesc.COMBINATION_CATEGORY, 
       LovbudgetcustomerView.KOMBINASI_CODE, 
       LovbudgetcustomerView.BUDGET_TYPE]]>
