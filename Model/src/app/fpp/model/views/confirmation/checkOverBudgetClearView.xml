  <SQLQuery>
    <![CDATA[SELECT Proposal.PROPOSAL_ID, 
        PROMPROD.PROMO_PRODUK_ID,
        ppidref.PPID_REF,
        ppidref.PROMO_PRODUK_ID AS PROMO_PRODUK_ID_TO,
        PROMPROD.PRODUCT_APPROVAL,
        PRODBUDBY.BUDGET_BY_ID,
       PRODBUDBY.BUDGET_CUST_ID,
        PRODBUDBY.AMOUNT,
        PRODBUDBY.OVER_BUDGET_AMT,
        PRODBUDBY.STATUS,
        PRODBUDBY.FLAG_STATUS_OVER,
       Proposal.PROPOSAL_NO,
       Proposal.CONFIRM_DATE, 
       Proposal.CONFIRM_NO, 
       Proposal.ADDENDUM_KE, 
       Proposal.CATEGORY_PC
FROM PROPOSAL Proposal, promo_produk promprod, prod_budget_by prodBudBy,GETPPIDREFFBUDGET ppidref
where  Proposal.CONFIRM_NO =:confirmNO
and nvl(Proposal.ADDENDUM_KE,0) =  (SELECT MAX(NVL(PROP.ADDENDUM_KE,0)) FROM PROPOSAL PROP WHERE PROP.CONFIRM_NO = Proposal.CONFIRM_NO)-1
and PROPOSAL.PROPOSAL_ID = promprod.PROPOSAL_ID
and PRODBUDBY.STATUS = 'Y'
and PRODBUDBY.FLAG_STATUS_OVER = 'Y'
and PROMPROD.PROMO_PRODUK_ID = PRODBUDBY.PROMO_PRODUK_ID(+)
and PROMPROD.PROMO_PRODUK_ID = ppidref.PPID_REF]]>
  </SQLQuery>
