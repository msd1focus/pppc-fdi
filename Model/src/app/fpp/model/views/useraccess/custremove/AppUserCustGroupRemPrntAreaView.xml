<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppUserCustGroupRemPrntAreaView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="AppUserArea.AREA_CODE, 
       AppUserArea.USER_NAME"
  FromList="APP_USER_AREA AppUserArea"
  Where="AppUserArea.USER_NAME = :userName
AND NOT EXISTS (
        SELECT 1
        FROM APPS.AR_CUSTOMERS AC, 
             APP_USER_LOC AUL,
             APP_USER_CUST_TYPE ACT,
             APP_USER_CUST_GROUP ACG
        WHERE ACG.USER_NAME = :userName
        AND AC.ATTRIBUTE4 = AppUserArea.AREA_CODE
        AND AC.ATTRIBUTE5 = AUL.LOCATION_CODE(+)
        AND AC.ATTRIBUTE8 = ACT.CUST_TYPE(+)
        AND AC.ATTRIBUTE1 = ACG.CUST_GROUP
)"
  RowClass="app.fpp.model.views.useraccess.custremove.AppUserCustGroupRemPrntAreaViewRowImpl"
  ComponentClass="app.fpp.model.views.useraccess.custremove.AppUserCustGroupRemPrntAreaViewImpl">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.useraccess.custremove.AppUserCustGroupRemPrntAreaView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="AppUserArea"
    Entity="app.fpp.model.entities.useraccess.AppUserArea"/>
  <ViewAttribute
    Name="AreaCode"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="AreaCode"
    EntityUsage="AppUserArea"
    AliasName="AREA_CODE"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="AppUserArea"
    AliasName="USER_NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
