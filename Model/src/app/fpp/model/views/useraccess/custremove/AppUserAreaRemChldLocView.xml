<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppUserAreaRemChldLocView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="AppUserLoc.LOCATION_CODE, 
       AppUserLoc.USER_NAME"
  FromList="APP_USER_LOC AppUserLoc, APPS.AR_CUSTOMERS ArCustomers"
  Where="AppUserLoc.LOCATION_CODE = ArCustomers.ATTRIBUTE5
AND ArCustomers.ATTRIBUTE4 NOT IN (SELECT AUA.AREA_CODE FROM APP_USER_AREA AUA WHERE AUA.USER_NAME = :userName)
AND AppUserLoc.USER_NAME = :userName
GROUP BY AppUserLoc.LOCATION_CODE, AppUserLoc.USER_NAME"
  BindingStyle="OracleName"
  CustomQuery="false"
  RowClass="app.fpp.model.views.useraccess.custremove.AppUserAreaRemChldLocViewRowImpl"
  ComponentClass="app.fpp.model.views.useraccess.custremove.AppUserAreaRemChldLocViewImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.useraccess.custremove.AppUserAreaRemChldLocView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="AppUserLoc"
    Entity="app.fpp.model.entities.useraccess.AppUserLoc"/>
  <EntityUsage
    Name="ArCustomers"
    Entity="app.fpp.model.entities.masterdata.ebs.ArCustomers"
    SourceUsage="app.fpp.model.views.useraccess.custremove.AppUserAreaRemChldLocView.AppUserLoc"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="LocationCode"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LocationCode"
    EntityUsage="AppUserLoc"
    AliasName="LOCATION_CODE"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="AppUserLoc"
    AliasName="USER_NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="app.fpp.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
