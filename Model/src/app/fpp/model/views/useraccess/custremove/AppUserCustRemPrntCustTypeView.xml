    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="app.fpp.model.views.useraccess.custremove.AppUserCustRemPrntCustTypeView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userName"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="AppUserCustType"
    Entity="app.fpp.model.entities.useraccess.AppUserCustType"/>
  <ViewAttribute
    Name="CustType"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CustType"
    EntityUsage="AppUserCustType"
    AliasName="CUST_TYPE"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
