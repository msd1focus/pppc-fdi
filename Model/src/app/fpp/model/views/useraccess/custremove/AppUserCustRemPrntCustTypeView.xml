<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AppUserCustRemPrntCustTypeView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="AppUserCustType.CUST_TYPE, 
       AppUserCustType.USER_NAME"
  FromList="APP_USER_CUST_TYPE AppUserCustType"
  Where="AppUserCustType.USER_NAME = :userName
AND NOT EXISTS (
        SELECT 1
        FROM APPS.AR_CUSTOMERS AC,
             APP_USER_CUST_GROUP ACG,
             APP_USER_CUST AUS
        WHERE AUS.USER_NAME = :userName
        AND AC.ATTRIBUTE8 = AppUserCustType.CUST_TYPE
        AND AC.ATTRIBUTE1 = ACG.CUST_GROUP(+)
        AND AC.CUSTOMER_ID = AUS.CUSTOMER_ID
)"
  RowClass="app.fpp.model.views.useraccess.custremove.AppUserCustRemPrntCustTypeViewRowImpl"
  ComponentClass="app.fpp.model.views.useraccess.custremove.AppUserCustRemPrntCustTypeViewImpl">
