<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="UserAccessView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  SelectList="UserAccess.ID, 
       UserAccess.USER_NAME, 
       UserAccess.PASSWORD, 
       UserAccess.FULL_NAME, 
       UserAccess.DESCR, 
       UserAccess.TITLE, 
       UserAccess.CONTACT_NO, 
       UserAccess.COMPANY_ID, 
       UserAccess.USER_TYPE, 
       (CASE WHEN UserAccess.ACTIVE_PERIOD_FROM > TO_DATE(TO_CHAR(SYSDATE, 'DD/MM/YYYY'), 'DD/MM/YYYY') OR UserAccess.ACTIVE_PERIOD_TO &lt; TO_DATE(TO_CHAR(SYSDATE, 'DD/MM/YYYY'), 'DD/MM/YYYY') THEN 'D' ELSE 'A' END) AS ACCESS_STATUS, 
       UserAccess.USER_INITIAL, 
       UserAccess.USER_DIVISION, 
       (FcsUserCustPriv.USER_CUSTOMER) AS USER_CUSTOMER, 
       (FcsUserCustPriv.USER_CUST_PRIV) AS USER_CUST_PRIV, 
       (LookupCodeRepDomExt.VALUE) AS REPORT_DOMAIN, 
       (LookupCodeRepDomLoc.VALUE) AS REPORT_DOMAIN_LOC, 
       (AppRoles.ADDENDUM_AREA) AS COPY_PP_AREA, 
       (AppUserLogged.USER_NAME) AS USER_LOGGED, 
       (LookupCodeSessionTime.VALUE) AS SESSION_CLR_TIME, 
       (AppRoles.PRINT_COUNT) AS PRINT_COUNT, 
       (AppRoles.UNAPPROVED) AS UN_APPROVED, 
       (AppRoles.APPROVAL_BUDGET) AS APPROVAL_BUDGET, 
       (AppRoles.BUDGET_STATUS) AS BUDGET_STATUS, 
       (LookupCodePrintMode.VALUE) AS PRINT_MODE, 
       (CASE WHEN NVL(LookupBatchUpload.VALUE, '-') &lt;> '-' THEN 'A' ELSE 'M' END) AS UPLOAD_USER"
  FromList="APP_USER_ACCESS UserAccess, FCS_USER_CUST_PRIV FcsUserCustPriv, APP_ROLES AppRoles, APP_USER_ACCESS_ROLES AppUserAccessRoles, APP_USER_LOGGED AppUserLogged, LOOKUP_CODE LookupCodeRepDomExt, LOOKUP_CODE LookupCodeRepDomLoc, LOOKUP_CODE LookupCodeSessionTime, LOOKUP_CODE LookupCodePrintMode, LOOKUP_CODE LookupBatchUpload"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  Where="UPPER(UserAccess.USER_NAME) = UPPER(:userLogin)
AND UPPER(UserAccess.PASSWORD) = UPPER(:userPassword)
AND FcsUserCustPriv.USER_NAME(+) = UserAccess.USER_NAME
AND AppRoles.NAME = AppUserAccessRoles.ROLE 
AND AppUserAccessRoles.USER_NAME = UserAccess.USER_NAME 
AND LookupCodeRepDomExt.TITLE = 'APP_REPORT_DOMAIN'
AND LookupCodeRepDomLoc.TITLE = 'APP_REPORT_DOMAIN_LOC'
AND LookupCodeSessionTime.TITLE = 'APP_CLEAR_SESSION'
AND LookupCodePrintMode.TITLE = 'APP_PRINT_MODE'
AND LookupBatchUpload.TITLE = 'BATCH_UPLOAD_USER'
AND AppUserLogged.USER_NAME(+) = UserAccess.USER_NAME
AND ROWNUM=1">
  <Variable
    Name="userLogin"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="userPassword"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="UserAccess"
    Entity="app.fpp.model.entities.useraccessmenu.UserAccess"/>
  <EntityUsage
    Name="FcsUserCustPriv"
    Entity="app.fpp.model.entities.useraccess.FcsUserCustPriv"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.UserAccess"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="AppRoles"
    Entity="app.fpp.model.entities.useraccess.AppRoles"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.FcsUserCustPriv"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="AppUserAccessRoles"
    Entity="app.fpp.model.entities.useraccess.AppUserAccessRoles"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.AppRoles"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="AppUserLogged"
    Entity="app.fpp.model.entities.useraccess.AppUserLogged"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.AppUserAccessRoles"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="LookupCodeRepDomExt"
    Entity="app.fpp.model.entities.masterdata.LookupCode"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.AppUserLogged"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="LookupCodeRepDomLoc"
    Entity="app.fpp.model.entities.masterdata.LookupCode"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.LookupCodeRepDomExt"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="LookupCodeSessionTime"
    Entity="app.fpp.model.entities.masterdata.LookupCode"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.LookupCodeRepDomLoc"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="LookupCodePrintMode"
    Entity="app.fpp.model.entities.masterdata.LookupCode"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.LookupCodeSessionTime"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <EntityUsage
    Name="LookupBatchUpload"
    Entity="app.fpp.model.entities.masterdata.LookupCode"
    SourceUsage="app.fpp.model.views.useraccessmenu.UserAccessView.LookupCodePrintMode"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="UserAccess"
    AliasName="ID"/>
  <ViewAttribute
    Name="UserName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserName"
    EntityUsage="UserAccess"
    AliasName="USER_NAME"/>
  <ViewAttribute
    Name="Password"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Password"
    EntityUsage="UserAccess"
    AliasName="PASSWORD"/>
  <ViewAttribute
    Name="FullName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="FullName"
    EntityUsage="UserAccess"
    AliasName="FULL_NAME"/>
  <ViewAttribute
    Name="Descr"
    PrecisionRule="true"
    EntityAttrName="Descr"
    EntityUsage="UserAccess"
    AliasName="DESCR"/>
  <ViewAttribute
    Name="Title"
    PrecisionRule="true"
    EntityAttrName="Title"
    EntityUsage="UserAccess"
    AliasName="TITLE"/>
  <ViewAttribute
    Name="ContactNo"
    PrecisionRule="true"
    EntityAttrName="ContactNo"
    EntityUsage="UserAccess"
    AliasName="CONTACT_NO"/>
  <ViewAttribute
    Name="CompanyId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CompanyId"
    EntityUsage="UserAccess"
    AliasName="COMPANY_ID"/>
  <ViewAttribute
    Name="UserType"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserType"
    EntityUsage="UserAccess"
    AliasName="USER_TYPE"/>
  <ViewAttribute
    Name="AccessStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ACCESS_STATUS"
    SQLType="VARCHAR"
    Precision="255"
    Expression="(CASE WHEN UserAccess.ACTIVE_PERIOD_FROM > TO_DATE(TO_CHAR(SYSDATE, 'DD/MM/YYYY'), 'DD/MM/YYYY') OR UserAccess.ACTIVE_PERIOD_TO &lt; TO_DATE(TO_CHAR(SYSDATE, 'DD/MM/YYYY'), 'DD/MM/YYYY') THEN 'D' ELSE 'A' END)"/>
  <ViewAttribute
    Name="UserInitial"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserInitial"
    EntityUsage="UserAccess"
    AliasName="USER_INITIAL"/>
  <ViewAttribute
    Name="UserDivision"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserDivision"
    EntityUsage="UserAccess"
    AliasName="USER_DIVISION"/>
  <ViewAttribute
    Name="UserCustomer"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USER_CUSTOMER"
    SQLType="VARCHAR"
    Precision="255"
    Expression="FcsUserCustPriv.USER_CUSTOMER"/>
  <ViewAttribute
    Name="UserCustPriv"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USER_CUST_PRIV"
    SQLType="VARCHAR"
    Precision="255"
    Expression="FcsUserCustPriv.USER_CUST_PRIV"/>
  <ViewAttribute
    Name="ReportDomain"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REPORT_DOMAIN"
    SQLType="VARCHAR"
    Precision="255"
    Expression="LookupCodeRepDomExt.VALUE"/>
  <ViewAttribute
    Name="ReportDomainLoc"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="REPORT_DOMAIN_LOC"
    SQLType="VARCHAR"
    Precision="255"
    Expression="LookupCodeRepDomLoc.VALUE"/>
  <ViewAttribute
    Name="CopyPpArea"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="COPY_PP_AREA"
    SQLType="VARCHAR"
    Precision="1"
    Expression="AppRoles.ADDENDUM_AREA"/>
  <ViewAttribute
    Name="UserLogged"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="USER_LOGGED"
    SQLType="VARCHAR"
    Precision="30"
    Expression="AppUserLogged.USER_NAME"/>
  <ViewAttribute
    Name="SessionClrTime"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SESSION_CLR_TIME"
    SQLType="VARCHAR"
    Precision="10"
    Expression="LookupCodeSessionTime.VALUE"/>
  <ViewAttribute
    Name="PrintCount"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRINT_COUNT"
    SQLType="VARCHAR"
    Precision="1"
    Expression="(AppRoles.PRINT_COUNT)"/>
  <ViewAttribute
    Name="UnApprove"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UN_APPROVED"
    SQLType="VARCHAR"
    Precision="1"
    Expression="(AppRoles.UNAPPROVED)"/>
  <ViewAttribute
    Name="ApprovalBudget"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="APPROVAL_BUDGET"
    SQLType="VARCHAR"
    Precision="1"
    Expression="(AppRoles.APPROVAL_BUDGET)"/>
  <ViewAttribute
    Name="BudgetStatus"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BUDGET_STATUS"
    SQLType="VARCHAR"
    Precision="1"
    Expression="(AppRoles.BUDGET_STATUS)"/>
  <ViewAttribute
    Name="PrintMode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRINT_MODE"
    SQLType="VARCHAR"
    Precision="5"
    Expression="LookupCodePrintMode.VALUE"/>
  <ViewAttribute
    Name="UploadUser"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="UPLOAD_USER"
    SQLType="VARCHAR"
    Precision="1"
    Expression="(CASE WHEN NVL(LookupBatchUpload.VALUE, '-') &lt;> '-' THEN 'A' ELSE 'M' END)"/>
</ViewObject>
